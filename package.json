  "scripts": {
    "start": "concurrently --raw \"npm run fes-local-token-dev\" \"npm run dev\"",
    "dev": "nx dev flowmanagementui",
    "build": "npx nx build flowmanagementui",
    "build:serve": "npm run build && node ./apps/flowmanagementui/.next/standalone/apps/flowmanagementui/server.js",
    "test": "nx run-many --targets=test",
    "test:coverage": "nx run-many --targets=test --codeCoverage && ts-node ./tooling/src/cli/fes-coverage-merge/index.ts",
    "lint": "nx run-many -t lint -p shared-i18n,shared-hooks,shared-store,shared-theme,shared-theme-dark,shared-theme-light,shared-utilities,shared-api,shared-components,fes-tooling",
    "typecheck": "nx run-many --targets=typecheck",
    "format": "nx run-many --targets=format",
    "validate": "npm run reset && npm run lint && npm run test && npm run format && npm run typecheck && npm run build",
    "copy-static": "ncp ./apps/flowmanagementui/.next/static ./apps/flowmanagementui/.next/standalone/apps/flowmanagementui/.next/static",
    "nx": "nx",
    "reset": "nx reset",
    "graph": "nx graph",
    "commitlint": "commitlint --edit",
    "sync-icons": "ts-node ./tooling/src/cli/mds-icon-sync/index.ts --distribution apps/flowmanagementui/.next/static --source apps,shared",
    "update-packages": "npx npm-check-updates -u --interactive",
    "prepare": "husky install",
    "postbuild": "npm run sync-icons && npm run copy-static",
    "mds-install": "ts-node ./tools/vsc-extensions/install.ts",
    "ts-node": "ts-node",
    "fes-local-token": "ts-node ./tooling/src/cli/fes-local-token/index.ts --envFiles apps/flowmanagementui/.env.local,apps/flowmanagementui/.env.production.local --envVariable NEXT_PUBLIC_ACCESS_TOKEN",
    "fes-local-token-dev": "ts-node ./tooling/src/cli/fes-local-token/index.ts --envFiles apps/flowmanagementui/.env.local,apps/flowmanagementui/.env.production.local --envVariable NEXT_PUBLIC_ACCESS_TOKEN --watch --interval 900000 --quiet",
    "postinstall": "ts-node ./tooling/src/cli/fes-installer/index.ts && npm run mds-install"
  },
